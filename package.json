    "prettier:check": "prettier -l './src/**/*.ts'"
  },
  "keywords": [
    "ng2",
    "ng",
    "angular",
    "angular2",
    "angularjs",
    "pipes",
    "filters",
    "library",
    "angular2 pipes",
    "ng2 pipes",
    "angular filters",
    "angular pipes"
