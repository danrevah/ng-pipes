{
  "name": "ngx-pipes",
  "version": "2.3.0",
  "author": "Dan Revah",
  "description": "Useful angular2 pipes",
  "license": "MIT",
  "scripts": {
    "start": "ng serve",
    "test": "ng test",
    "test-travis": "ng test -cc --watch=false",
    "build": "ng build",
    "build:lib": "ng-packagr",
